{
  "name": "olajoscs/nestable-generator",
  "description": "A json generator and parser for the nestable structure editor",
  "minimum-stability": "stable",
  "homepage": "https://github.com/olajoscs/nestable",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Csaba Olajos",
      "email": "olajoscs@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "OlajosCs\\Nestable\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OlajosCs\\Nestable\\": "./tests"
    }
  },
  "require": {
    "php": "~7.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5.0",
    "infection/infection": "^0.13.4",
    "phpstan/phpstan": "^0.11.12"
  }
}
